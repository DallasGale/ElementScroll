"eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": [
      "airbnb-base"
    ],
    "env": {
      "node": true,
      "browser": true
    },
    "plugins": [
      "compat",
      "html",
      "import"
    ],
    "rules": {
      "no-prototype-builtins": 0,
      "no-continue": 0,
      "prefer-template": 0,
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "no-restricted-syntax": 0,
      "no-else-return": 0,
      "no-underscore-dangle": 0,
      "wrap-iife": [
        "error",
        "inside"
      ],
      "prefer-spread": 0,
      "guard-for-in": 0,
      "prefer-rest-params": 0,
      "dot-notation": 0,
      "camelcase": 0,
      "func-names": [
        "error",
        "never"
      ],
      "class-methods-use-this": "off",
      "max-len": 0,
      "lines-between-class-members": 0,
      "function-paren-newline": 0,
      "prefer-destructuring": 0,
      "no-plusplus": 0,
      "global-require": 0,
      "import/no-unresolved": 0,
      "no-param-reassign": 0,
      "no-shadow": 0,
      "import/extensions": 0,
      "import/newline-after-import": 0,
      "no-multi-assign": 0
    }
  }